              "categories": {
                "additionalProperties": {
                  "description": "Plant performance assessment category.",
                  "properties": {
                    "category_results": {
                      "description": "Category level assessment results.",
                      "items": {
                        "$ref": "#/definitions/Result"
                      },
                      "title": "Category Results",
                      "type": "array"
                    },
                    "components": {
                      "description": "Plant performance assessment category components.",
                      "items": {
                        "description": "Plant performance assessment component.",
                        "properties": {
                          "basis": {
                            "description": "Basis of plant performance element assessment.",
