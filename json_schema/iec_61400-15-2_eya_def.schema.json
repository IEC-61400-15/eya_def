{
  "$id": "https://raw.githubusercontent.com/IEC-61400-15/eya_def/blob/main/iec_61400-15-2_eya_def.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$version": "0.0.1",
  "definitions": {
    "CalculationModelSpecification": {
      "description": "Specification of a model used in an energy assessment.",
      "properties": {
        "comments": {
          "description": "Comments on the model.",
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the model.",
          "title": "Description",
          "type": "string"
        },
        "name": {
          "description": "Name of the model.",
          "examples": [
            "WAsP",
            "VORTEX BLOCKS",
            "DNV CFD",
            "VENTOS/M"
          ],
          "title": "Name",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "CalculationModelSpecification",
      "type": "object"
    },
    "CoordinateReferenceSystem": {
      "description": "Specification of a coordinate reference system for GIS data.",
      "properties": {
        "epsg_srid": {
          "description": "EPSG Spatial Reference System Identifier (SRID).",
          "examples": [
            27700,
            3006
          ],
          "title": "Epsg Srid",
          "type": "integer"
        },
        "system_label": {
          "description": "Label of the coordinate system.",
          "examples": [
            "OSGB36 / British National Grid",
            "SWEREF99 TM"
          ],
          "title": "System Label",
          "type": "string"
        },
        "wkt": {
          "description": "Well-known text (WKT) string definition of the coordinate system.",
          "title": "Wkt",
          "type": "string"
        }
      },
      "required": [
        "system_label"
      ],
      "title": "CoordinateReferenceSystem",
      "type": "object"
    },
    "GrossEnergyAssessment": {
      "description": "Gross energy yield assessment.",
      "properties": {
        "results": {
          "description": "Gross energy yield results.",
          "items": {
            "$ref": "#/definitions/Results"
          },
          "title": "Results",
          "type": "array"
        }
      },
      "required": [
        "results"
      ],
      "title": "GrossEnergyAssessment",
      "type": "object"
    },
    "Location": {
      "description": "Specification of a horizontal location in space.",
      "properties": {
        "comments": {
          "description": "Comments regarding the location.",
          "examples": [
            "Pending ground investigations"
          ],
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the location.",
          "examples": [
            "Preliminary location of T1"
          ],
          "title": "Description",
          "type": "string"
        },
        "label": {
          "description": "Label of the location.",
          "examples": [
            "T1",
            "WTG02-loc1",
            "WEA_003/R2"
          ],
          "title": "Label",
          "type": "string"
        },
        "location_id": {
          "description": "Unique identifier of the location.",
          "examples": [
            "ee15ff84-6733-4858-9656-ba995d9b1022",
            "WTG02-loc1"
          ],
          "title": "Location Id",
          "type": "string"
        },
        "x": {
          "description": "Location x-coordinate (typically easing).",
          "examples": [
            419665.0
          ],
          "title": "X",
          "type": "number"
        },
        "y": {
          "description": "Location y-coordinate (typically northing).",
          "examples": [
            6195240.0
          ],
          "title": "Y",
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ],
      "title": "Location",
      "type": "object"
    },
    "NetEnergyAssessment": {
      "description": "Net energy yield results.",
      "properties": {
        "results": {
          "description": "Energy yield results at different confidence limits.",
          "items": {
            "$ref": "#/definitions/Results"
          },
          "title": "Results",
          "type": "array"
        },
        "uncertainty_assessment": {
          "$ref": "#/definitions/UncertaintyAssessment",
          "description": "Net energy uncertainty assessment.",
          "title": "Uncertainty Assessment"
        }
      },
      "required": [
        "results"
      ],
      "title": "NetEnergyAssessment",
      "type": "object"
    },
    "OperationalRestriction": {
      "description": "Specifications of an operational restriction.",
      "properties": {
        "comments": {
          "description": "Comments regarding the operational restriction.",
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the operational restriction.",
          "examples": [
            "Wind sector management (WSM) curtailment as specified by the turbine manufacturer"
          ],
          "title": "Description",
          "type": "string"
        },
        "label": {
          "description": "Label of the operational restriction.",
          "examples": [
            "WSM curtailment",
            "MEC curtailment"
          ],
          "title": "Label",
          "type": "string"
        }
      },
      "required": [
        "label",
        "description"
      ],
      "title": "OperationalRestriction",
      "type": "object"
    },
    "Organisation": {
      "description": "Issuing or receiving organisation of an energy yield assessment.",
      "properties": {
        "abbreviation": {
          "description": "Abbreviated name of the organisation.",
          "examples": [
            "Torre Egger",
            "Miranda"
          ],
          "title": "Abbreviation",
          "type": "string"
        },
        "address": {
          "description": "Address of the organisation.",
          "examples": [
            "5 Munro Road, Summit Centre, Sgurrsville, G12 0YE, UK"
          ],
          "title": "Address",
          "type": "string"
        },
        "contact_name": {
          "description": "Name(s) of contact person(s) in the organisation.",
          "examples": [
            "Luis Bunuel",
            "Miles Davis, John Coltrane"
          ],
          "title": "Contact Name",
          "type": "string"
        },
        "name": {
          "description": "Entity name of the organisation.",
          "examples": [
            "The Torre Egger Consultants Limited",
            "Miranda Investments Limited"
          ],
          "title": "Name",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "Organisation",
      "type": "object"
    },
    "PlantPerformanceAssessment": {
      "description": "Plant performance assessment details and results.",
      "properties": {
        "categories": {
          "additionalProperties": {
            "$ref": "#/definitions/PlantPerformanceCategory"
          },
          "description": "Plant performance assessment categories.",
          "title": "Categories",
          "type": "object"
        }
      },
      "required": [
        "categories"
      ],
      "title": "PlantPerformanceAssessment",
      "type": "object"
    },
    "PlantPerformanceCategory": {
      "description": "Plant performance assessment category.",
      "properties": {
        "category_results": {
          "description": "Category level assessment results.",
          "items": {
            "$ref": "#/definitions/Results"
          },
          "title": "Category Results",
          "type": "array"
        },
        "components": {
          "description": "Plant performance assessment category components.",
          "items": {
            "$ref": "#/definitions/PlantPerformanceComponent"
          },
          "title": "Components",
          "type": "array"
        }
      },
      "required": [
        "components",
        "category_results"
      ],
      "title": "PlantPerformanceCategory",
      "type": "object"
    },
    "PlantPerformanceComponent": {
      "description": "Plant performance assessment component.",
      "properties": {
        "basis": {
          "description": "Basis of plant performance element assessment.",
          "enum": [
            "timeseries_calculation",
            "distribution_calculation",
            "other_calculation",
            "project_specific_estimate",
            "regional_assumption",
            "generic_assumption",
            "not_considered",
            "other"
          ],
          "title": "Basis",
          "type": "string"
        },
        "calculation_models": {
          "description": "Calculation models used in the assessment.",
          "items": {
            "$ref": "#/definitions/CalculationModelSpecification"
          },
          "title": "Calculation Models",
          "type": "array"
        },
        "results": {
          "$ref": "#/definitions/Results",
          "description": "Plant performance assessment component results.",
          "title": "Results"
        },
        "variability": {
          "description": "Considered variability in plant performance element.",
          "enum": [
            "static_process",
            "annual_variable",
            "other"
          ],
          "title": "Variability",
          "type": "string"
        }
      },
      "required": [
        "basis",
        "variability",
        "results"
      ],
      "title": "PlantPerformanceComponent",
      "type": "object"
    },
    "ReferenceWindFarm": {
      "description": "Reference wind farm metadata.",
      "properties": {},
      "title": "ReferenceWindFarm",
      "type": "object"
    },
    "ReportContributor": {
      "description": "Contributor to an energy yield assessment.",
      "properties": {
        "completion_date": {
          "description": "Contribution completion date (format YYYY-MM-DD).",
          "examples": [
            "2022-10-04"
          ],
          "format": "date",
          "title": "Completion Date",
          "type": "string"
        },
        "contribution_comments": {
          "description": "Comments to clarify contribution.",
          "examples": [
            "Second author"
          ],
          "title": "Contribution Comments",
          "type": "string"
        },
        "contributor_type": {
          "description": "Type of contributor.",
          "enum": [
            "author",
            "verifier",
            "approver",
            "other"
          ],
          "title": "Contributor Type",
          "type": "string"
        },
        "email_address": {
          "description": "Email address of the contributor.",
          "examples": [
            "j.miro@art.cat",
            "andrei.tarkovsky@cinema.com"
          ],
          "format": "email",
          "title": "Email Address",
          "type": "string"
        },
        "name": {
          "description": "Name of the contributor.",
          "examples": [
            "Joan Miro",
            "Andrei Tarkovsky"
          ],
          "title": "Name",
          "type": "string"
        }
      },
      "required": [
        "name",
        "contributor_type"
      ],
      "title": "ReportContributor",
      "type": "object"
    },
    "Results": {
      "description": "Single set of results for an element of an energy assessment.",
      "properties": {
        "applicability_type": {
          "description": "Applicability type of energy assessment results.",
          "enum": [
            "lifetime",
            "any_one_year",
            "one_operational_year",
            "other"
          ],
          "title": "Applicability Type",
          "type": "string"
        },
        "comments": {
          "description": "Comments on the results.",
          "examples": [
            "Corresponds to first 10 years of operation."
          ],
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the results.",
          "examples": [
            "10-year wind farm net P50 energy yield."
          ],
          "title": "Description",
          "type": "string"
        },
        "label": {
          "description": "Label of the results.",
          "examples": [
            "10-year P50"
          ],
          "title": "Label",
          "type": "string"
        },
        "result_components": {
          "description": "List of result components.",
          "items": {
            "$ref": "#/definitions/ResultsComponent"
          },
          "title": "Result Components",
          "type": "array"
        },
        "results_dimensions": {
          "description": "Type of energy assessment results.",
          "items": {
            "enum": [
              "none",
              "location",
              "hub_height",
              "year",
              "month",
              "month_of_year"
            ],
            "type": "string"
          },
          "title": "Results Dimensions",
          "type": "array"
        },
        "unit": {
          "description": "Unit of result values (TO REPLACE BY LITERAL).",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "label",
        "unit",
        "applicability_type",
        "results_dimensions",
        "result_components"
      ],
      "title": "Results",
      "type": "object"
    },
    "ResultsComponent": {
      "description": "Component of a set of results.",
      "properties": {
        "comments": {
          "description": "Comments on the results component.",
          "title": "Comments",
          "type": "string"
        },
        "component_type": {
          "description": "Type of results component.",
          "examples": [
            "mean",
            "median",
            "std",
            "P90"
          ],
          "title": "Component Type",
          "type": "string"
        },
        "description": {
          "description": "Description of the results component.",
          "title": "Description",
          "type": "string"
        },
        "values": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "additionalProperties": {
                "type": "number"
              },
              "type": "object"
            },
            {
              "additionalProperties": {
                "additionalProperties": {
                  "type": "number"
                },
                "type": "object"
              },
              "type": "object"
            },
            {
              "additionalProperties": {
                "additionalProperties": {
                  "additionalProperties": {
                    "type": "number"
                  },
                  "type": "object"
                },
                "type": "object"
              },
              "type": "object"
            },
            {
              "additionalProperties": {
                "additionalProperties": {
                  "additionalProperties": {
                    "additionalProperties": {
                      "type": "number"
                    },
                    "type": "object"
                  },
                  "type": "object"
                },
                "type": "object"
              },
              "type": "object"
            }
          ],
          "description": "Result value(s) as simple float or labeled map.",
          "examples": [
            123.4,
            {
              "WTG01": 123.4,
              "WTG02": 143.2
            }
          ],
          "title": "Values"
        }
      },
      "required": [
        "component_type",
        "values"
      ],
      "title": "ResultsComponent",
      "type": "object"
    },
    "Scenario": {
      "description": "Single unique energy yield assessment scenario.",
      "properties": {
        "comments": {
          "description": "Comments on the scenario.",
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the scenario.",
          "examples": [
            "Main scenario",
            "XZY220-7.2MW turbine model scenario"
          ],
          "title": "Description",
          "type": "string"
        },
        "gross_energy_assessment": {
          "$ref": "#/definitions/GrossEnergyAssessment",
          "description": "Gross energy yield assessment.",
          "title": "Gross Energy Assessment"
        },
        "is_main_scenario": {
          "description": "Whether or not this is the main scenario in the report.",
          "title": "Is Main Scenario",
          "type": "boolean"
        },
        "label": {
          "description": "Label of the scenario.",
          "examples": [
            "Sc1",
            "A",
            "B01"
          ],
          "title": "Label",
          "type": "string"
        },
        "net_energy_assessment": {
          "$ref": "#/definitions/NetEnergyAssessment",
          "description": "Net energy yield assessment.",
          "title": "Net Energy Assessment"
        },
        "operational_lifetime_length_years": {
          "description": "Number of years of project operational lifetime.",
          "examples": [
            10.0,
            20.0,
            30.0
          ],
          "exclusiveMaximum": 100.0,
          "exclusiveMinimum": 1.0,
          "title": "Operational Lifetime Length Years",
          "type": "number"
        },
        "plant_performance_assessment": {
          "$ref": "#/definitions/PlantPerformanceAssessment",
          "description": "Plant performance assessment including all losses.",
          "title": "Plant Performance Assessment"
        },
        "scenario_id": {
          "description": "Unique identifier of the scenario.",
          "examples": [
            "3613a846-1e74-4535-ad40-7368f7ad452d"
          ],
          "title": "Scenario Id",
          "type": "string"
        },
        "turbine_wind_resource_assessment": {
          "$ref": "#/definitions/TurbineWindResourceAssessment",
          "description": "Wind resource assessment at the turbine locations.",
          "title": "Turbine Wind Resource Assessment"
        },
        "wind_farms": {
          "default": [],
          "description": "List of all wind farms included in the scenario.",
          "items": {
            "$ref": "#/definitions/WindFarm"
          },
          "title": "Wind Farms",
          "type": "array"
        },
        "wind_resource_assessment_basis": {
          "$ref": "#/definitions/WindResourceAssessmentBasis",
          "description": "Measurement wind resource assessment basis for the scenario.",
          "title": "Wind Resource Assessment Basis"
        }
      },
      "required": [
        "label",
        "operational_lifetime_length_years",
        "gross_energy_assessment",
        "plant_performance_assessment",
        "net_energy_assessment"
      ],
      "title": "Scenario",
      "type": "object"
    },
    "TurbineModel": {
      "description": "Specification of a wind turbine model.",
      "properties": {
        "label": {
          "description": "Label of the turbine model.",
          "examples": [
            "V172-7.2 MW",
            "N175/6.X",
            "SG 6.6-170",
            "E-175 EP5"
          ],
          "title": "Label",
          "type": "string"
        },
        "perf_spec_ref": {
          "$ref": "https://foo.bar.com/baz/wtg_model.schema.json",
          "description": "Reference to a json document with turbine model performance specification (PLACEHOLDER).",
          "examples": [
            "https://foo.com/bar/example_wtg_model.json"
          ],
          "title": "Turbine Model Performance Specification Reference",
          "type": "string"
        },
        "turbine_model_id": {
          "description": "Unique identifier of the turbine model.",
          "examples": [
            "dbb25743-60f4-4eab-866f-31d5f8af69d6",
            "XYZ199-8.5MW v004"
          ],
          "title": "Turbine Model Id",
          "type": "string"
        }
      },
      "required": [
        "turbine_model_id",
        "label"
      ],
      "title": "TurbineModel",
      "type": "object"
    },
    "TurbineSpecification": {
      "description": "Specification of all details for a turbine configuration.",
      "properties": {
        "comments": {
          "description": "Comments regarding the turbine configuration.",
          "examples": [
            "Preliminary alternative configuration with Unconfirmed turbine suitability"
          ],
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the turbine configuration.",
          "examples": [
            "Turbine specification for T1 in Scenario B"
          ],
          "title": "Description",
          "type": "string"
        },
        "hub_height": {
          "description": "Turbine hub height.",
          "title": "Hub Height",
          "type": "number"
        },
        "label": {
          "description": "Label of the turbine.",
          "examples": [
            "T1",
            "WTG02",
            "WEA_003"
          ],
          "title": "Label",
          "type": "string"
        },
        "location": {
          "$ref": "#/definitions/Location",
          "description": "Horizontal location of the turbine.",
          "title": "Location"
        },
        "restrictions": {
          "description": "List of operational restrictions at the turbine level.",
          "items": {
            "$ref": "#/definitions/OperationalRestriction"
          },
          "title": "Restrictions",
          "type": "array"
        },
        "turbine_id": {
          "description": "Unique identifier of the turbine specification.",
          "examples": [
            "b55caeac-f152-4b13-8217-3fddeab792cf",
            "T1-scen1"
          ],
          "title": "Turbine Id",
          "type": "string"
        },
        "turbine_model_id": {
          "description": "Unique identifier of the turbine model.",
          "title": "Turbine Model Id",
          "type": "string"
        }
      },
      "required": [
        "label",
        "location",
        "hub_height",
        "turbine_model_id"
      ],
      "title": "TurbineSpecification",
      "type": "object"
    },
    "TurbineWindResourceAssessment": {
      "description": "Wind resource assessment at the turbine locations.",
      "properties": {
        "turbine_wind_resource_results": {
          "description": "Assessment results at the turbine location(s).",
          "items": {
            "$ref": "#/definitions/Results"
          },
          "title": "Turbine Wind Resource Results",
          "type": "array"
        },
        "uncertainty_assessment": {
          "$ref": "#/definitions/UncertaintyAssessment",
          "description": "Turbine wind resource uncertainty assessment.",
          "title": "Uncertainty Assessment"
        },
        "wind_spatial_models": {
          "description": "Wind spatial models used in the assessment.",
          "items": {
            "$ref": "#/definitions/CalculationModelSpecification"
          },
          "title": "Wind Spatial Models",
          "type": "array"
        }
      },
      "required": [
        "turbine_wind_resource_results",
        "wind_spatial_models"
      ],
      "title": "TurbineWindResourceAssessment",
      "type": "object"
    },
    "UncertaintyAssessment": {
      "description": "Wind resource uncertainty assessment including all components.",
      "properties": {
        "categories": {
          "additionalProperties": {
            "$ref": "#/definitions/UncertaintyCategory"
          },
          "description": "Wind resource uncertainty assessment categories.",
          "title": "Categories",
          "type": "object"
        }
      },
      "required": [
        "categories"
      ],
      "title": "UncertaintyAssessment",
      "type": "object"
    },
    "UncertaintyCategory": {
      "description": "Wind resource uncertainty assessment category.",
      "properties": {
        "category_results": {
          "description": "Category level assessment results.",
          "items": {
            "$ref": "#/definitions/Results"
          },
          "title": "Category Results",
          "type": "array"
        },
        "components": {
          "description": "Wind resource uncertainty assessment components.",
          "items": {
            "$ref": "#/definitions/UncertaintyComponent"
          },
          "title": "Components",
          "type": "array"
        }
      },
      "required": [
        "components",
        "category_results"
      ],
      "title": "UncertaintyCategory",
      "type": "object"
    },
    "UncertaintyComponent": {
      "description": "Wind resource uncertainty assessment component.",
      "properties": {
        "comments": {
          "description": "Comments on the wind uncertainty component.",
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the wind uncertainty component.",
          "examples": [
            "Uncertainty associated with the consistency of the long-term reference data"
          ],
          "title": "Description",
          "type": "string"
        },
        "label": {
          "description": "Label of the wind uncertainty component.",
          "examples": [
            "Long-term consistency"
          ],
          "title": "Label",
          "type": "string"
        },
        "results": {
          "$ref": "#/definitions/Results",
          "description": "Wind resource uncertainty assessment component results.",
          "title": "Results"
        }
      },
      "required": [
        "label",
        "results"
      ],
      "title": "UncertaintyComponent",
      "type": "object"
    },
    "WindFarm": {
      "description": "A collection of wind turbines considered as one unit (plant).",
      "properties": {
        "comments": {
          "description": "Comments regarding the wind farm.",
          "title": "Comments",
          "type": "string"
        },
        "description": {
          "description": "Description of the wind farm.",
          "examples": [
            "The third phase of the Summit project"
          ],
          "title": "Description",
          "type": "string"
        },
        "label": {
          "description": "Abbreviated label of the wind farm.",
          "examples": [
            "BWF",
            "Summit PhIII"
          ],
          "title": "Label",
          "type": "string"
        },
        "name": {
          "description": "Name of the wind farm.",
          "examples": [
            "Barefoot Wind Farm",
            "Project Summit Phase III"
          ],
          "title": "Name",
          "type": "string"
        },
        "operational_lifetime_end_date": {
          "description": "Operational lifetime end date (format YYYY-MM-DD).",
          "examples": [
            "2051-03-31",
            "2025-12-31"
          ],
          "format": "date",
