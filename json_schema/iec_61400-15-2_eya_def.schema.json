                "comments": {
                  "$ref": "#/definitions/CommentsField"
                },
                "description": {
                  "$ref": "#/definitions/DescriptionField"
                },
                "label": {
                  "description": "Abbreviated label of the wind farm.",
                  "examples": [
                    "BWF",
                    "Summit PhIII"
                  ],
                  "title": "Label",
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "name": {
                  "description": "Name of the wind farm.",
                  "examples": [
                    "Barefoot Wind Farm",
                    "Project Summit Phase III"
                  ],
                  "title": "Name",
                  "type": "string"
                },
                "operational_lifetime_end_date": {
                  "description": "Operational lifetime end date (format YYYY-MM-DD).",
                  "examples": [
                    "2051-03-31",
                    "2025-12-31"
