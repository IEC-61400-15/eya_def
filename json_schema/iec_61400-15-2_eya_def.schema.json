                              "timeseries_calculation",
                              "distribution_calculation",
                              "other_calculation",
                              "project_specific_estimate",
                              "regional_assumption",
                              "generic_assumption",
                              "not_considered",
                              "other"
                            ],
                            "title": "Assessment Basis",
                            "type": "string"
                          },
                          "calculation_models": {
                            "description": "Calculation models used in the assessment.",
                            "items": {
                              "$ref": "#/definitions/CalculationModelSpecification"
                            },
                            "title": "Calculation Models",
                            "type": [
                              "array",
                              "null"
                            ]
                          },
                          "results": {
                            "$ref": "#/definitions/Result",
                            "description": "Plant performance assessment component results.",
                            "title": "Results"
                          },
                          "variability": {
                            "description": "Considered variability in plant performance element.",
                            "enum": [
                              "static_process",
                              "annual_variable",
                              "other"
                            ],
                            "title": "Variability Type",
                            "type": "string"
                          }
                        },
                        "required": [
                          "basis",
                          "variability",
                          "results"
                        ],
                        "title": "Plant Performance Component",
                        "type": "object"
                      },
                      "title": "Components",
                      "type": "array"
                    }
                  },
                  "required": [
                    "components",
                    "category_results"
                  ],
                  "title": "Plant Performance Category",
                  "type": "object"
                },
                "description": "Plant performance assessment categories.",
                "title": "Categories",
                "type": "object"
              },
              "net_energy_results": {
                "description": "Net energy yield results at different confidence limits.",
                "items": {
                  "$ref": "#/definitions/Result"
                },
                "title": "Net Energy Results",
                "type": "array"
              },
              "net_energy_uncertainty_assessment": {
                "$ref": "#/definitions/UncertaintyAssessment",
                "description": "Net energy uncertainty assessment.",
                "title": "Net Energy Uncertainty Assessment"
              }
            },
            "required": [
              "categories",
              "net_energy_results"
            ],
            "title": "Plant Performance Assessment",
            "type": "object"
          },
          "scenario_id": {
            "description": "Unique identifier of the scenario.",
            "examples": [
              "3613a846-1e74-4535-ad40-7368f7ad452d"
            ],
            "title": "Scenario Id",
            "type": [
              "string",
              "null"
            ]
          },
          "turbine_wind_resource_assessment": {
            "description": "Wind resource assessment at the turbine locations.",
            "properties": {
              "turbine_wind_resource_results": {
                "description": "Assessment results at the turbine location(s).",
                "items": {
                  "$ref": "#/definitions/Result"
                },
                "title": "Turbine Wind Resource Results",
                "type": "array"
              },
              "uncertainty_assessment": {
                "$ref": "#/definitions/UncertaintyAssessment",
                "description": "Turbine wind resource uncertainty assessment.",
                "title": "Uncertainty Assessment"
              },
              "wind_spatial_models": {
                "description": "Wind spatial models used in the assessment.",
                "items": {
                  "$ref": "#/definitions/CalculationModelSpecification"
                },
                "title": "Wind Spatial Models",
                "type": "array"
              }
            },
            "required": [
              "turbine_wind_resource_results",
              "wind_spatial_models"
            ],
            "title": "Turbine Wind Resource Assessment",
            "type": "object"
          },
