{
  "$id": "https://raw.githubusercontent.com/IEC-61400-15/eya_def/blob/main/iec_61400-15-2_eya_def.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$version": "0.0.1",
  "additionalProperties": false,
  "definitions": {
    "CalculationModelSpecification": {
      "description": "Specification of a model used in an energy assessment.",
      "properties": {
        "comments": {
          "$ref": "#/definitions/CommentsField"
        },
        "description": {
          "$ref": "#/definitions/DescriptionField"
        },
        "name": {
          "description": "Name of the model.",
          "examples": [
            "WAsP",
            "VORTEX BLOCKS",
            "DNV CFD",
            "VENTOS/M"
          ],
          "title": "Name",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "CalculationModelSpecification",
      "type": "object"
    },
    "CommentsField": {
      "description": "Comments on the data object.",
      "title": "Comments",
      "type": [
        "string",
        "null"
      ]
    },
    "DescriptionField": {
      "description": "Description of the data object.",
      "title": "Description",
      "type": [
        "string",
        "null"
      ]
    },
    "OperationalRestriction": {
      "description": "Specifications of an operational restriction.",
      "properties": {
        "comments": {
          "$ref": "#/definitions/CommentsField"
        },
        "description": {
          "$ref": "#/definitions/DescriptionField"
        },
        "label": {
          "description": "Label of the operational restriction.",
          "examples": [
            "WSM curtailment",
            "MEC curtailment"
          ],
          "title": "Label",
          "type": "string"
        }
      },
      "required": [
        "label"
      ],
      "title": "OperationalRestriction",
      "type": "object"
    },
    "Organisation": {
      "description": "Issuing or receiving organisation of an energy yield assessment.",
      "properties": {
        "abbreviation": {
          "description": "Abbreviated name of the organisation.",
          "examples": [
            "Torre Egger",
            "Miranda"
          ],
          "title": "Abbreviation",
          "type": [
            "string",
            "null"
          ]
        },
        "address": {
          "description": "Address of the organisation.",
          "examples": [
            "5 Munro Road, Summit Centre, Sgurrsville, G12 0YE, UK"
          ],
          "title": "Address",
          "type": [
            "string",
            "null"
          ]
        },
        "contact_name": {
          "description": "Name(s) of contact person(s) in the organisation.",
          "examples": [
            "Luis Bunuel",
            "Miles Davis, John Coltrane"
          ],
          "title": "Contact Name",
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "description": "Entity name of the organisation.",
          "examples": [
            "The Torre Egger Consultants Limited",
            "Miranda Investments Limited"
          ],
          "title": "Name",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "Organisation",
      "type": "object"
    },
    "Result": {
      "description": "Set of results for an element of an energy assessment.",
      "properties": {
        "assessment_period": {
          "default": "lifetime",
          "description": "Period of or in time that has been assessed and for which the results are applicable.",
          "enum": [
            "lifetime",
            "any_one_year",
            "one_operational_year",
            "other"
          ],
          "examples": [
            "lifetime",
            "any_one_year"
          ],
          "title": "Assessment Period",
          "type": "string"
        },
        "comments": {
          "$ref": "#/definitions/CommentsField"
        },
        "description": {
          "$ref": "#/definitions/DescriptionField"
        },
        "dimensions": {
          "description": "Dimensions along which the results are binned (all result values in the same results object must have the same dimensions).",
          "examples": [
            [
              "turbine",
              "year"
            ],
            [
              "measurement",
              "height"
            ]
          ],
          "items": {
            "description": "Dimension along which results are assigned.",
            "enum": [
              "height",
              "hour",
              "measurement",
              "month",
              "turbine",
              "year"
            ],
            "title": "Results Dimension",
            "type": "string"
          },
          "type": [
            "array",
            "null"
          ]
        },
        "label": {
          "description": "Label of the results.",
          "examples": [
            "Seasonal distribution of net energy."
          ],
          "title": "Label",
          "type": [
            "string",
            "null"
          ]
        },
        "statistics": {
          "description": "List of result statistic objects that each include result values for a specific statistic type.",
          "items": {
            "description": "Result values for one specific statistic type.",
            "properties": {
              "comments": {
                "$ref": "#/definitions/CommentsField"
              },
              "description": {
                "$ref": "#/definitions/DescriptionField"
              },
              "statistic_type": {
                "description": "Type of statistic in the results component.",
                "enum": [
                  "mean",
                  "median",
                  "standard_deviation",
                  "minimum",
                  "maximum",
                  "p10",
                  "p25",
                  "p75",
                  "p90",
                  "p99"
                ],
                "examples": [
                  "median",
                  "p90"
                ],
                "title": "Statistic Type",
                "type": "string"
              },
              "values": {
                "anyOf": [
                  {
                    "type": "number"
                  },
                  {
                    "items": {
                      "items": false,
                      "maxItems": 2,
                      "minItems": 2,
                      "prefixItems": [
                        {
                          "items": {
                            "anyOf": [
                              {
                                "type": "number"
                              },
                              {
                                "type": "integer"
                              },
                              {
                                "type": "string"
                              }
                            ]
                          },
                          "type": "array"
                        },
                        {
                          "type": "number"
                        }
                      ],
                      "type": "array"
                    },
                    "type": "array"
                  }
                ],
                "description": "Result as a single number or values at coordinates.",
                "title": "Values"
              }
            },
            "required": [
              "statistic_type",
              "values"
            ],
            "title": "Result Statistic",
            "type": "object"
          },
          "title": "Statistics",
          "type": "array"
        }
      },
      "required": [
        "label",
        "statistics"
      ],
      "title": "Result",
      "type": "object"
    },
    "UncertaintyAssessment": {
      "description": "Wind resource uncertainty assessment including all components.",
      "properties": {
        "categories": {
          "additionalProperties": {
            "description": "Wind resource uncertainty assessment category.",
            "properties": {
              "category_results": {
                "description": "Category level assessment results.",
                "items": {
                  "$ref": "#/definitions/Result"
                },
                "title": "Category Results",
                "type": "array"
              },
              "components": {
                "description": "Wind resource uncertainty assessment components.",
                "items": {
                  "description": "Wind resource uncertainty assessment component.",
                  "properties": {
                    "comments": {
                      "$ref": "#/definitions/CommentsField"
                    },
                    "description": {
                      "$ref": "#/definitions/DescriptionField"
                    },
                    "label": {
                      "description": "Label of the wind uncertainty component.",
                      "examples": [
                        "Long-term consistency"
                      ],
                      "title": "Label",
                      "type": "string"
                    },
                    "results": {
                      "description": "Wind resource uncertainty assessment component results.",
                      "items": {
                        "$ref": "#/definitions/Result"
                      },
                      "title": "Results",
                      "type": "array"
                    }
                  },
                  "required": [
                    "label",
                    "results"
                  ],
                  "title": "Uncertainty Component",
                  "type": "object"
                },
                "title": "Components",
                "type": "array"
              }
            },
            "required": [
              "components",
              "category_results"
            ],
            "title": "Uncertainty Category",
            "type": "object"
          },
          "description": "Wind resource uncertainty assessment categories.",
          "title": "Categories",
          "type": "object"
        }
      },
      "required": [
        "categories"
      ],
      "title": "UncertaintyAssessment",
      "type": "object"
    }
  },
  "description": "IEC 61400-15-2 EYA DEF energy yield assessment data model.",
  "properties": {
    "$id": {
      "description": "Unique URI of the JSON document.",
      "examples": [
        "https://foo.com/api/eya?id=8f46a815-8b6d-4870-8e92-c031b20320c6.json"
      ],
      "title": "ID",
      "type": [
        "string",
        "null"
      ]
    },
    "comments": {
      "$ref": "#/definitions/CommentsField"
    },
    "confidentiality_classification": {
      "description": "Confidentiality classification of the report.",
      "examples": [
        "Strictly confidential",
        "Commercial in confidence",
        "Public"
      ],
      "title": "Confidentiality Classification",
      "type": [
        "string",
        "null"
      ]
    },
    "contract_reference": {
      "description": "Reference to contract between the issuing and receiving organisations that governs the energy yield assessment.",
      "examples": [
        "Contract ID.: P-MIR-00239432-0001-C, dated 2022-11-30"
      ],
      "title": "Contract Reference",
      "type": [
        "string",
        "null"
      ]
    },
    "contributors": {
      "description": "List of report contributors (e.g. author and verifier)",
      "items": {
        "description": "Contributor to an energy yield assessment.",
        "properties": {
          "completion_date": {
            "description": "Contribution completion date (format YYYY-MM-DD).",
            "examples": [
              "2022-10-04"
            ],
            "format": "date",
            "title": "Completion Date",
            "type": [
              "string",
              "null"
            ]
          },
          "contribution_comments": {
            "description": "Comments to clarify contribution.",
            "examples": [
              "Second author"
            ],
            "title": "Contribution Comments",
            "type": [
              "string",
              "null"
            ]
          },
          "contributor_type": {
            "description": "Type of contributor.",
            "enum": [
              "author",
              "verifier",
              "approver",
              "other"
            ],
            "title": "Contributor Type",
            "type": "string"
          },
          "email_address": {
            "description": "Email address of the contributor.",
            "examples": [
              "j.miro@art.cat",
              "andrei.tarkovsky@cinema.com"
            ],
            "format": "email",
            "title": "Email Address",
            "type": [
              "string",
              "null"
            ]
          },
          "name": {
            "description": "Name of the contributor.",
            "examples": [
              "Joan Miro",
              "Andrei Tarkovsky"
            ],
            "title": "Name",
            "type": "string"
          }
        },
        "required": [
          "name",
          "contributor_type"
        ],
        "title": "Report Contributor",
        "type": "object"
      },
      "title": "Contributors",
      "type": "array"
    },
    "coordinate_reference_system": {
      "description": "Coordinate reference system used for all location data.",
      "properties": {
        "epsg_srid": {
          "description": "EPSG Spatial Reference System Identifier (SRID).",
          "examples": [
            27700,
            3006
          ],
          "title": "Epsg Srid",
          "type": [
            "integer",
            "null"
          ]
        },
        "system_label": {
          "description": "Label of the coordinate system.",
          "examples": [
            "OSGB36 / British National Grid",
            "SWEREF99 TM"
          ],
          "title": "System Label",
          "type": "string"
        },
        "wkt": {
          "description": "Well-known text (WKT) string definition of the coordinate system.",
          "title": "Wkt",
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "system_label"
      ],
      "title": "Coordinate Reference System",
      "type": "object"
    },
    "description": {
      "$ref": "#/definitions/DescriptionField"
    },
    "document_id": {
      "description": "The ID of the report document; not including the version when 'document_version' is used.",
      "examples": [
        "C385945/A/UK/R/002",
        "0345.923454.0001"
      ],
      "title": "Document ID",
      "type": [
        "string",
        "null"
      ]
    },
    "document_version": {
      "description": "Version of the report document, also known as revision.",
      "examples": [
        "1.2.3",
        "A",
        "Rev. A"
      ],
      "title": "Document Version",
      "type": [
        "string",
        "null"
      ]
    },
    "issue_date": {
      "description": "Report issue date in the ISO 8601 standard format for a calendar date, i.e. YYYY-MM-DD.",
      "examples": [
        "2022-10-05"
      ],
      "format": "date",
      "title": "Issue Date",
      "type": "string"
    },
    "issuing_organisations": {
      "description": "The organisation(s) issuing the report (e.g. consultant).",
      "items": {
        "$ref": "#/definitions/Organisation"
      },
      "title": "Issuing Organisations",
      "type": "array"
    },
    "measurement_stations": {
      "description": "List of measurement station metadata JSON document(s) according to the IEA Task 43 WRA Data Model.",
      "items": {
        "$ref": "https://raw.githubusercontent.com/IEA-Task-43/digital_wra_data_standard/master/schema/iea43_wra_data_model.schema.json",
        "description": "A measurement metadata JSON document according to the IEA Task 43 WRA Data Model.",
        "examples": [
          "https://foo.com/bar/example_iea43.json"
        ]
      },
      "title": "Measurement Stations",
      "type": "array"
    },
    "project_name": {
      "description": "Name of the project under assessment.",
      "examples": [
        "Barefoot Wind Farm"
      ],
      "title": "Project Name",
      "type": "string"
    },
    "receiving_organisations": {
      "description": "The organisation(s) receiving the report (e.g. client), if relevant.",
      "items": {
        "$ref": "#/definitions/Organisation"
      },
      "title": "Receiving Organisations",
      "type": [
        "array",
        "null"
      ]
    },
    "reference_wind_farms": {
      "description": "List of reference operational wind farms.",
      "items": {
        "description": "Reference wind farm metadata.",
        "properties": {},
        "title": "Reference Wind Farm",
        "type": "object"
      },
      "title": "Reference Wind Farms",
      "type": [
        "array",
        "null"
      ]
    },
    "scenarios": {
      "description": "List of energy yield assessment scenarios.",
      "items": {
        "description": "Single unique energy yield assessment scenario.",
        "properties": {
          "comments": {
            "$ref": "#/definitions/CommentsField"
          },
          "description": {
            "$ref": "#/definitions/DescriptionField"
          },
          "gross_energy_assessment": {
            "description": "Gross energy yield assessment.",
            "properties": {
              "results": {
                "description": "Gross energy yield results.",
                "items": {
                  "$ref": "#/definitions/Result"
                },
                "title": "Results",
                "type": "array"
              }
            },
            "required": [
              "results"
            ],
            "title": "Gross Energy Assessment",
            "type": "object"
          },
          "is_main_scenario": {
            "description": "Whether or not this is the main scenario in the report.",
            "title": "Is Main Scenario",
            "type": [
              "boolean",
              "null"
            ]
          },
          "label": {
            "description": "Label of the scenario.",
            "examples": [
              "Sc1",
              "A",
              "B01"
            ],
            "title": "Label",
            "type": "string"
          },
          "operational_lifetime_length_years": {
            "description": "Number of years of project operational lifetime.",
            "examples": [
              10.0,
              20.0,
              30.0
            ],
            "exclusiveMaximum": 100.0,
            "exclusiveMinimum": 1.0,
            "title": "Operational Lifetime Length Years",
            "type": "number"
          },
          "plant_performance_assessment": {
            "description": "Plant performance assessment including net energy results.",
